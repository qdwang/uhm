{
    "name": "uhm-extension",
    "displayName": "uhm Wavetable Extension",
    "description": "Extension for uhm wavetable script in (u-he Software).",
    "version": "0.1.0",
    "publisher": "qdwang",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:uhm"
    ],
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "uhm",
                "aliases": [
                    "uhm",
                    "uhm"
                ],
                "extensions": [
                    ".uhm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "uhm",
                "scopeName": "source.uhm",
                "path": "./syntaxes/uhm.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/qdwang/uhm-extension.git"
    }
}
