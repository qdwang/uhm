{
    "name": "uhm-extension",
    "displayName": "uhm Wavetable Extension",
    "description": "Extension for uhm wavetable script in (u-he Software).",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "uhm",
                "aliases": [
                    "uhm",
                    "uhm"
                ],
                "extensions": [
                    ".uhm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "uhm",
                "scopeName": "source.uhm",
                "path": "./syntaxes/uhm.tmLanguage.json"
            }
        ]
    }
}
